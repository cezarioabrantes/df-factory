{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "cabrantesdatafactory"
		},
		"appdb_service_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'appdb_service'"
		},
		"appvm_server_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'appvm_server'"
		},
		"cabrantesdatalake_service_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'cabrantesdatalake_service'"
		},
		"cabrantesworkspace_sqlpool_service_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'cabrantesworkspace_sqlpool_service'"
		},
		"appvm_server_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "D:\\nginx-1.26.1\\logs"
		},
		"appvm_server_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "cabrantes"
		},
		"cabrantesdatalake_service_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://cabrantesdatalakegen2.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "cabrantesdatafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"appdb_service_connectionString": {
						"value": "[parameters('appdb_service_connectionString')]"
					},
					"cabrantesdatalake_service_accountKey": {
						"value": "[parameters('cabrantesdatalake_service_accountKey')]"
					},
					"cabrantesworkspace_sqlpool_service_connectionString": {
						"value": "[parameters('cabrantesworkspace_sqlpool_service_connectionString')]"
					},
					"cabrantesdatalake_service_properties_typeProperties_url": {
						"value": "[parameters('cabrantesdatalake_service_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "cabrantesdatafactory_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"appvm_server_password": {
						"value": "[parameters('appvm_server_password')]"
					},
					"appvm_server_properties_typeProperties_host": {
						"value": "[parameters('appvm_server_properties_typeProperties_host')]"
					},
					"appvm_server_properties_typeProperties_userId": {
						"value": "[parameters('appvm_server_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'cabrantesdatafactory_ArmTemplate_0')]"
			]
		},
		{
			"name": "cabrantesdatafactory_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'cabrantesdatafactory_ArmTemplate_1')]"
			]
		},
		{
			"name": "cabrantesdatafactory_ArmTemplate_3",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_3.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'cabrantesdatafactory_ArmTemplate_2')]"
			]
		}
	]
}